{
    "expo": {
        "name": "Navigation App",
        "slug": "navigation-app",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "cover",
            "backgroundColor": "#379FFF"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "bundleIdentifier": "com.mypackage.coolapp",
            "googleServicesFile": "./GoogleService-Info.plist",
            "config": {
                "googleMapsApiKey": "AIzaSyCKiN1FuOVkvn9NalCY4HcI7YFEDnJwFNM"
            },
            "supportsTablet": true
        },
        "android": {
            "package": "br.com.navigation",
            "googleServicesFile": "./google-services.json",
            "config": {
                "googleMaps": {
                    "apiKey": "AIzaSyCKiN1FuOVkvn9NalCY4HcI7YFEDnJwFNM"
                }
            },
            "adaptiveIcon": {
                "foregroundImage": "./assets/icon.png",
                "backgroundColor": "#379FFF"
            }
        },
        "web": {
            "favicon": "./assets/favicon.png"
        }
    }
}